<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="18.5dp"
        android:fontFamily="@font/open_sans_bold"
        android:text="Rating &amp; reviews"
        android:textStyle="bold"
        android:textColor="@color/black7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/review_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="14dp"
        android:layout_marginRight="16dp"
        android:text="number of review"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/rating_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:text="5"
        android:layout_marginEnd="4dp"
        app:layout_constraintEnd_toStartOf="@+id/course_rating"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:ignore="UnknownId"
        android:layout_marginRight="4dp" />

    <RatingBar
        android:id="@+id/course_rating"
        style="?android:attr/ratingBarStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:isIndicator="true"
        android:numStars="5"
        android:layout_marginStart="4dp"
        android:progressTint="@color/gold"
        android:stepSize="0.1"
        app:layout_constraintBottom_toBottomOf="@+id/rating_number"
        app:layout_constraintStart_toEndOf="@+id/rating_number"
        app:layout_constraintTop_toTopOf="@+id/rating_number"
        tools:ignore="SpeakableTextPresentCheck"
        android:layout_marginLeft="4dp" />

    <ProgressBar
        android:id="@+id/simpleProgressBar"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="235dp"
        android:layout_height="15dp"
        android:max="100"
        android:padding="5dp"
        android:progress="50"
        app:layout_constraintBottom_toBottomOf="@+id/course_rating"
        app:layout_constraintStart_toEndOf="@+id/course_rating"
        app:layout_constraintTop_toTopOf="@+id/course_rating"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/rating_number1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:text="4"
        android:layout_marginEnd="4dp"
        app:layout_constraintEnd_toStartOf="@+id/course_rating1"
        app:layout_constraintStart_toStartOf="@+id/rating_number"
        app:layout_constraintTop_toBottomOf="@+id/rating_number"

        tools:ignore="UnknownId"
        android:layout_marginRight="4dp" />

    <RatingBar
        android:id="@+id/course_rating1"
        style="?android:attr/ratingBarStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:isIndicator="true"
        android:numStars="5"
        android:layout_marginStart="4dp"
        android:progressTint="@color/gold"
        android:stepSize="0.1"
        app:layout_constraintBottom_toBottomOf="@+id/rating_number1"
        app:layout_constraintStart_toEndOf="@+id/rating_number1"
        app:layout_constraintTop_toTopOf="@+id/rating_number1"
        tools:ignore="SpeakableTextPresentCheck"
        android:layout_marginLeft="4dp" />

    <ProgressBar
        android:id="@+id/simpleProgressBar1"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="235dp"
        android:layout_height="15dp"
        android:max="100"
        android:padding="5dp"
        android:progress="40"
        app:layout_constraintBottom_toBottomOf="@+id/course_rating1"
        app:layout_constraintStart_toEndOf="@+id/course_rating1"
        app:layout_constraintTop_toTopOf="@+id/course_rating1"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/rating_number2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:text="3"
        android:layout_marginEnd="4dp"
        app:layout_constraintEnd_toStartOf="@+id/course_rating2"
        app:layout_constraintStart_toStartOf="@+id/rating_number1"
        app:layout_constraintTop_toBottomOf="@+id/rating_number1"

        tools:ignore="UnknownId"
        android:layout_marginRight="4dp" />

    <RatingBar
        android:id="@+id/course_rating2"
        style="?android:attr/ratingBarStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:isIndicator="true"
        android:numStars="5"
        android:layout_marginStart="4dp"
        android:progressTint="@color/gold"
        android:stepSize="0.1"
        app:layout_constraintBottom_toBottomOf="@+id/rating_number2"
        app:layout_constraintStart_toEndOf="@+id/rating_number2"
        app:layout_constraintTop_toTopOf="@+id/rating_number2"
        tools:ignore="SpeakableTextPresentCheck"
        android:layout_marginLeft="4dp" />

    <ProgressBar
        android:id="@+id/simpleProgressBar2"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="235dp"
        android:layout_height="15dp"
        android:max="100"
        android:padding="5dp"
        android:progress="30"
        app:layout_constraintBottom_toBottomOf="@+id/course_rating2"
        app:layout_constraintStart_toEndOf="@+id/course_rating2"
        app:layout_constraintTop_toTopOf="@+id/course_rating2"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/rating_number3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:text="2"
        android:layout_marginEnd="4dp"
        app:layout_constraintEnd_toStartOf="@+id/course_rating3"
        app:layout_constraintStart_toStartOf="@+id/rating_number2"
        app:layout_constraintTop_toBottomOf="@+id/rating_number2"

        tools:ignore="UnknownId"
        android:layout_marginRight="4dp" />

    <RatingBar
        android:id="@+id/course_rating3"
        style="?android:attr/ratingBarStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:isIndicator="true"
        android:numStars="5"
        android:layout_marginStart="4dp"
        android:progressTint="@color/gold"
        android:stepSize="0.1"
        app:layout_constraintBottom_toBottomOf="@+id/rating_number3"
        app:layout_constraintStart_toEndOf="@+id/rating_number3"
        app:layout_constraintTop_toTopOf="@+id/rating_number3"
        tools:ignore="SpeakableTextPresentCheck"
        android:layout_marginLeft="4dp" />

    <ProgressBar
        android:id="@+id/simpleProgressBar3"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="235dp"
        android:layout_height="15dp"
        android:max="100"
        android:padding="5dp"
        android:progress="20"
        app:layout_constraintBottom_toBottomOf="@+id/course_rating3"
        app:layout_constraintStart_toEndOf="@+id/course_rating3"
        app:layout_constraintTop_toTopOf="@+id/course_rating3"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/rating_number4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:text="1"
        android:layout_marginEnd="4dp"
        app:layout_constraintEnd_toStartOf="@+id/course_rating4"
        app:layout_constraintStart_toStartOf="@+id/rating_number3"
        app:layout_constraintTop_toBottomOf="@+id/rating_number3"

        tools:ignore="UnknownId"
        android:layout_marginRight="4dp" />

    <RatingBar
        android:id="@+id/course_rating4"
        style="?android:attr/ratingBarStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:isIndicator="true"
        android:numStars="5"
        android:layout_marginStart="4dp"
        android:progressTint="@color/gold"
        android:stepSize="0.1"
        app:layout_constraintBottom_toBottomOf="@+id/rating_number4"
        app:layout_constraintStart_toEndOf="@+id/rating_number4"
        app:layout_constraintTop_toTopOf="@+id/rating_number4"
        tools:ignore="SpeakableTextPresentCheck"
        android:layout_marginLeft="4dp" />

    <ProgressBar
        android:id="@+id/simpleProgressBar4"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="235dp"
        android:layout_height="15dp"
        android:max="100"
        android:padding="5dp"
        android:progress="10"
        app:layout_constraintBottom_toBottomOf="@+id/course_rating4"
        app:layout_constraintStart_toEndOf="@+id/course_rating4"
        app:layout_constraintTop_toTopOf="@+id/course_rating4"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/state_line"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="36dp"
        android:layout_marginBottom="44dp"
        android:background="@color/light_black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rating_number4"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>